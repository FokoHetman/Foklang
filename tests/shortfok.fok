fokfetch = {
  enable = true;
  backend = "ascii";
  display = [
    "/========<{username}@{hostname}>========/"
    "OS: {os}"
    "/========<{blank-username}@{blank-hostname}>========/"
  ];
};


parsedisplay [] = [];
parsedisplay (c:display) = c : "\n" : (parsedisplay display);

cursorToData = "";


if (fokfetch.enable) {

  print cursorToData;
  println $ parsedisplay (fokfetch.display);
};
