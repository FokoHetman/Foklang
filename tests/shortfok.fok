fokfetch = {
  enable = true;
  backend = "ascii";
  display = [
    "/========<{username}@{hostname}>========/"
    "OS: {os}"
    "/========<{blank-username}@{blank-hostname}>========/"
  ];
};

parse_display [] = "";
parse_display (c:display) = c : "\n" : parse_display display;

cursorToData = "\0x1b[5C";

if fokfetch.enable {

  print cursorToData;
  println $ parse_display fokfetch.display;
}
