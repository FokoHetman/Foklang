fokfetch = {
  enable = true;
  backend = "ascii";
  display = [
    "/========<{username}@{hostname}>========/"
    "OS: {os}"

    "/========<{blank-username}@{blank-hostname}>========/"
  ];
};

parse_display:: [[char]] -> [char]
parse_display [] = "";
parse_display (c:display) = c ++ "\n" ++ parse_display display;

cursorToData = "\033[??"

if fokfetch.enable {
  os_data = parse_data $ read_file /etc/os-release;
  ascii = os_data
  print cursorToData;
  println $ parse_display fokfetch.display;
}
